<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.khadija.smartfridge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_refrigerator"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_refrigerator"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RecipesDetails.RecipieDetailsActivity"></activity>
        <activity android:name=".CamScreen" />
        <activity android:name=".contents.Contents"/>
        <activity android:name=".Recipes.RecipesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>